<VBox prefHeight="700.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mindpart.radio3.ui.MainController">
    <children>
        <AnchorPane fx:id="mainPane" VBox.vgrow="ALWAYS">
            <children>
                <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <tabs>
                  <Tab fx:id="deviceTab" text="Device">
                     <content>
                        <AnchorPane prefHeight="200.0" prefWidth="200.0">
                           <children>
                              <SplitPane dividerPositions="0.5" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                <items>
                                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
                                       <children>
                                          <VBox alignment="TOP_CENTER" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                             <children>
                                                <GridPane hgap="5.0" vgap="5.0">
                                                   <columnConstraints>
                                                      <ColumnConstraints hgrow="NEVER" />
                                                      <ColumnConstraints hgrow="ALWAYS" />
                                                   </columnConstraints>
                                                   <rowConstraints>
                                                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                                   </rowConstraints>
                                                   <children>
                                                      <Label text="Hardware Revision" />
                                                      <ChoiceBox fx:id="hardwareRevisions" prefWidth="150.0" GridPane.columnIndex="1" />
                                                      <Label text="VFO Type" GridPane.rowIndex="1" />
                                                      <ChoiceBox fx:id="vfoType" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                                                      <HBox alignment="CENTER_LEFT" spacing="5.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
                                                         <children>
                                                                <ChoiceBox fx:id="serialPorts" minWidth="212.0" prefHeight="27.0" prefWidth="212.0" />
                                                                <Button fx:id="serialPortsRefresh" focusTraversable="false" mnemonicParsing="false" text="Refresh" />
                                                                <Button fx:id="deviceConnect" defaultButton="true" mnemonicParsing="false" text="Connect" />
                                                         </children>
                                                      </HBox>
                                                      <Label text="Serial Port" GridPane.rowIndex="2" />
                                                   </children>
                                                   <padding>
                                                      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                                   </padding>
                                                </GridPane>
                                                <Separator prefWidth="200.0">
                                                   <VBox.margin>
                                                      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                                   </VBox.margin>
                                                </Separator>
                                                <TableView fx:id="devicePropertiesTable">
                                                   <columns>
                                                      <TableColumn editable="false" prefWidth="75.0" text="Property">
                                                         <cellValueFactory>
                                                            <PropertyValueFactory property="name" />
                                                         </cellValueFactory>
                                                      </TableColumn>
                                                      <TableColumn editable="false" sortable="false" text="Value">
                                                         <cellValueFactory>
                                                            <PropertyValueFactory property="value" />
                                                         </cellValueFactory>
                                                      </TableColumn>
                                                   </columns>
                                                   <columnResizePolicy>
                                                      <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                                   </columnResizePolicy>
                                                </TableView>
                                                <Button fx:id="devicePropertiesRefresh" mnemonicParsing="false" text="Refresh" />
                                             </children>
                                             <padding>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                             </padding>
                                          </VBox>
                                       </children>
                                    </AnchorPane>
                                  <AnchorPane>
                                       <children>
                                          <VBox fx:id="deviceRuntimePane" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                             <padding>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                             </padding>
                                             <children>
                                                <Label text="Device log:" />
                                                <ScrollPane prefHeight="200.0" prefWidth="200.0" VBox.vgrow="SOMETIMES">
                                                   <content>
                                                      <VBox fx:id="deviceLogBox" />
                                                   </content>
                                                </ScrollPane>
                                             </children>
                                          </VBox>
                                       </children>
                                    </AnchorPane>
                                </items>
                              </SplitPane>
                           </children>
                        </AnchorPane>
                     </content></Tab>
                        <Tab fx:id="sweepTab" text="Sweep Generator" />
                        <Tab fx:id="vnaTab" text="VNA" />
                        <Tab fx:id="componentsTab" text="Components">
                            <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                    <children>
                              <SplitPane dividerPositions="0.6" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                <items>
                                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
                                       <children>
                                                    <VBox fx:id="componentsBox" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                                        <children>
                                                <Label text="Probes and sampling">
                                                   <VBox.margin>
                                                      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                                   </VBox.margin>
                                                </Label>
                                                <Separator prefWidth="200.0" />
                                                            <HBox alignment="CENTER_LEFT" spacing="5.0">
                                                                <children>
                                                                    <Button fx:id="sampleAllProbesBtn" mnemonicParsing="false" onAction="#doSampleAllProbes" text="Get All" />
                                                                    <ToggleButton fx:id="continuousSamplingOfAllProbesBtn" mnemonicParsing="false" onAction="#doContinuousSamplingOfAllProbes" text="Continuous Sampling">
                                                                        <HBox.margin>
                                                                            <Insets />
                                                                        </HBox.margin>
                                                                    </ToggleButton>
                                                                    <Separator prefWidth="200.0" visible="false" />
                                                                </children>
                                                                <padding>
                                                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                                                </padding>
                                                            </HBox>
                                                        </children>
                                                    </VBox>
                                       </children>
                                    </AnchorPane>
                                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
                                       <children>
                                          <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                             <children>
                                                <Label text="Configuration">
                                                   <VBox.margin>
                                                      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                                   </VBox.margin>
                                                </Label>
                                                <Separator prefWidth="200.0" />
                                                <GridPane hgap="5.0" vgap="5.0">
                                                  <columnConstraints>
                                                    <ColumnConstraints hgrow="NEVER" />
                                                    <ColumnConstraints hgrow="SOMETIMES" />
                                                  </columnConstraints>
                                                  <rowConstraints>
                                                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                                  </rowConstraints>
                                                   <children>
                                                      <Label text="VFO output" GridPane.rowIndex="3" />
                                                      <Label text="VFO attenuator" GridPane.rowIndex="2" />
                                                      <ChoiceBox fx:id="vfoAttenuator" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                                                      <ChoiceBox fx:id="vfoOutput" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                                                      <Label text="VFO amplifier" GridPane.rowIndex="1" />
                                                      <Label text="VNA mode" />
                                                      <ChoiceBox fx:id="vfoAmplifier" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                                                      <ChoiceBox fx:id="vnaMode" prefWidth="150.0" GridPane.columnIndex="1" />
                                                   </children>
                                                   <padding>
                                                      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                                   </padding>
                                                </GridPane>
                                             </children>
                                          </VBox>
                                       </children>
                                    </AnchorPane>
                                </items>
                              </SplitPane>
                                    </children>
                                    <padding>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                    </padding>
                                </AnchorPane>
                            </content>
                        </Tab>
                    </tabs>
                </TabPane>
            </children>
        </AnchorPane>
      <Separator prefWidth="200.0" />
      <HBox alignment="CENTER_LEFT" spacing="5.0">
         <children>
            <Circle fx:id="mainIndicator" radius="8.0" stroke="#00000055" strokeType="INSIDE" HBox.hgrow="NEVER">
               <fill>
                  <RadialGradient centerX="0.449438202247191" centerY="0.40865384615384615" focusAngle="-36.87" focusDistance="-0.19047619047619058" radius="0.33333333333333337">
                     <stops>
                        <Stop color="WHITE" />
                        <Stop color="WHITE" offset="0.17472118959107802" />
                        <Stop color="#0cb50c" offset="0.6505576208178439" />
                        <Stop color="#0cb50c" offset="1.0" />
                     </stops>
                  </RadialGradient>
               </fill>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Circle>
                <Label fx:id="deviceConnectionStatus" maxWidth="1.7976931348623157E308" text="Label" HBox.hgrow="ALWAYS" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
    </children>
    <stylesheets>
        <URL value="@main.css" />
    </stylesheets>
</VBox>
