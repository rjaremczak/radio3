<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.cell.*?>

<VBox minHeight="600.0" minWidth="900.0" prefHeight="700.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mindpart.radio3.ui.MainController">
    <children>
        <SplitPane dividerPositions="0.75" prefHeight="532.0" prefWidth="900.0" VBox.vgrow="SOMETIMES">
            <items>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <children>
                          <ToolBar minHeight="40.0" minWidth="300.0" prefHeight="40.0" prefWidth="300.0">
                              <items>
                                  <Button fx:id="deviceSelectionRefresh" mnemonicParsing="false" onAction="#refreshAvailablePorts" text="Refresh" />
                                  <ChoiceBox fx:id="deviceSelection" minWidth="212.0" prefHeight="27.0" prefWidth="212.0" />
                                  <Button fx:id="deviceConnect" mnemonicParsing="false" onAction="#doConnectDisconnect" text="Connect" />
                              </items>
                          </ToolBar>
                     </children>
                  </VBox>
               </children></AnchorPane>
            <VBox prefWidth="100.0">
               <children>
                  <ToolBar prefHeight="40.0" prefWidth="200.0">
                     <items>
                        <Button fx:id="testBtn" mnemonicParsing="false" onAction="#doTest" text="Test" />
                     </items></ToolBar>
                  <TitledPane fx:id="deviceInfoPane" animated="false" expanded="false" prefWidth="274.0" text="Device Info">
                    <content>
                      <AnchorPane prefHeight="150.0">
                           <children>
                              <TableView fx:id="devicePropertiesTable" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                <columns>
                                  <TableColumn editable="false" prefWidth="75.0" text="Property">
                                      <cellValueFactory>
                                          <PropertyValueFactory property="name" />
                                      </cellValueFactory>
                                  </TableColumn>
                                  <TableColumn editable="false" sortable="false" text="Value">
                                      <cellValueFactory>
                                          <PropertyValueFactory property="value" />
                                      </cellValueFactory>
                                  </TableColumn>
                                </columns>
                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                              </TableView>
                           </children>
                        </AnchorPane>
                    </content>
                  </TitledPane>
                  <TitledPane fx:id="vfoPane" animated="false" text="VFO">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="90.0" prefWidth="200.0">
                           <children>
                              <GridPane hgap="5.0" prefHeight="90.0" prefWidth="272.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                <columnConstraints>
                                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="127.0" minWidth="10.0" prefWidth="83.0" />
                                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="183.0" minWidth="10.0" prefWidth="78.0" />
                                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="179.0" minWidth="10.0" prefWidth="70.0" />
                                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="179.0" minWidth="10.0" prefWidth="21.0" />
                                </columnConstraints>
                                <rowConstraints>
                                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                </rowConstraints>
                                 <children>
                                    <TextField fx:id="vfoFrequency" promptText="frequency" text="0" GridPane.columnIndex="1" GridPane.columnSpan="2" />
                                    <Label text="Frequency" />
                                    <Label text="Hz" GridPane.columnIndex="3" />
                                    <Button fx:id="vfoSetFrequency" mnemonicParsing="false" onAction="#doVfoSetFrequency" text="Set" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                                 </children>
                                 <padding>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </padding>
                              </GridPane>
                           </children></AnchorPane>
                    </content>
                  </TitledPane>
                  <TitledPane animated="false" text="Frequency Meter">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="90.0" prefWidth="200.0">
                           <children>
                              <GridPane hgap="5.0" prefHeight="90.0" prefWidth="272.0" vgap="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                <columnConstraints>
                                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="92.0" minWidth="10.0" prefWidth="84.0" />
                                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="148.0" minWidth="10.0" prefWidth="60.0" />
                                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="87.0" minWidth="1.0" prefWidth="85.0" />
                                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="87.0" minWidth="10.0" prefWidth="20.0" />
                                </columnConstraints>
                                <rowConstraints>
                                  <RowConstraints maxHeight="38.0" minHeight="10.0" prefHeight="38.0" vgrow="SOMETIMES" />
                                  <RowConstraints maxHeight="37.0" minHeight="10.0" prefHeight="37.0" vgrow="SOMETIMES" />
                                </rowConstraints>
                                 <children>
                                    <Label text="Frequency" />
                                    <TextField fx:id="freqMeterValue" editable="false" focusTraversable="false" promptText="frequency" text="0" GridPane.columnIndex="1" GridPane.columnSpan="2" />
                                    <Button fx:id="freqMeterRead" mnemonicParsing="false" onAction="#doFreqMeterRead" text="Read" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                                    <Label text="Hz" GridPane.columnIndex="3" />
                                    <ToggleButton fx:id="freqMeterPoll" mnemonicParsing="false" text="Poll" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                                 </children>
                                 <padding>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </padding>
                              </GridPane>
                           </children>
                        </AnchorPane>
                    </content>
                  </TitledPane>
               </children>
            </VBox>
            </items>
        </SplitPane>
      <ToolBar prefHeight="40.0" prefWidth="200.0">
         <items>
                <Label fx:id="deviceStatus" text="Label" />
         </items>
      </ToolBar>
    </children>
    <stylesheets>
        <URL value="@main.css" />
    </stylesheets>
</VBox>
