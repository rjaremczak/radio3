<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<AnchorPane minHeight="300.0" minWidth="650.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mindpart.radio3.ui.AnalyserController">
   <children>
      <VBox layoutX="211.0" layoutY="50.0" prefHeight="200.0" prefWidth="100.0" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox spacing="5.0">
               <children>
                  <Button fx:id="presetsButton" mnemonicParsing="false" onAction="#onPresets" text="Presets" />
                  <TextField fx:id="startFrequency" alignment="CENTER_RIGHT" focusTraversable="false" onAction="#onStartFrequency" prefHeight="27.0" prefWidth="80.0" promptText="start freq." />
                  <TextField fx:id="endFrequency" alignment="CENTER_RIGHT" focusTraversable="false" onAction="#onEndFrequency" prefHeight="27.0" prefWidth="80.0" promptText="end freq." />
                  <TextField fx:id="frequencyStep" alignment="CENTER_RIGHT" focusTraversable="false" onAction="#onFrequencyStep" prefHeight="27.0" prefWidth="80.0" promptText="step" />
                  <Button fx:id="startButton" mnemonicParsing="false" onAction="#doStart" text="Start" />
                  <Label fx:id="statusLabel" text="Label">
                     <HBox.margin>
                        <Insets top="5.0" />
                     </HBox.margin>
                  </Label>
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
            <LineChart fx:id="lineChart" VBox.vgrow="ALWAYS">
               <xAxis>
                  <CategoryAxis side="BOTTOM" />
               </xAxis>
               <yAxis>
                  <NumberAxis side="LEFT" />
               </yAxis>
            </LineChart>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
